{
    "name": "theme-black-dracula",
    "version": "1.0.0",
    "displayName": "Black Dracula Official",
    "homepage": "https://draculatheme.com/",
    "description": "Official Black Dracula Theme. A super dark theme for many editors, shells, and more.",
    "publisher": "black-dracula-theme",
    "license": "MIT",
    "scripts": {
        "build": "node ./scripts/build.js",
        "lint": "node ./scripts/lint.js",
        "package": "mkdir -p bin && vsce package -o ./bin/dracula.vsix",
        "vscode:prepublish": "npm run build",
        "vsce-publish": "vsce publish"
    },
    "maintainers": [
        "Lelzin"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/d3cryptofc/black-dracula-vscode"
    },
    "bugs": {
        "url": "https://github.com/dracula/visual-studio-code/issues"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "dracula",
        "dark",
        "theme",
        "color-theme",
        "black"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#3c4557",
        "theme": "dark"
    },
    "contributes": {
        "themes": [
            {
                "label": "Black Dracula",
                "uiTheme": "vs-dark",
                "path": "./theme/dracula.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0",
        "prettier": "^3.0.0",
        "tinycolor2": "^1.6.0",
        "vsce": "^2.15.0"
    },
    "__metadata": {
        "id": "8e03e570-8da4-43e5-b82f-6d64d4fd86a8",
        "publisherDisplayName": "Black Dracula Theme",
        "publisherId": "0657c56a-1a86-609a-a4ba-87f5493ea458"
    }
}
